<!-- Contact Section
==========================================-->
<div id="tf-contact">
  <div class="container">


      <div class="row">

        <div class="section-title center text-center">
          <h2><strong>contact info</strong></h2>
          <div class="line">
            <hr>
          </div>
          <div class="clearfix"></div>
          <small>
            <em>Reach out to us at Mjenzi to discuss how we can assist your business.</em></small>
        </div>


        <div class="col-md-6 contact-info">
          <h4><strong>contact us at Mjenzi</strong></h4>
          <ul>

            <li>
              <span class="glyphicon glyphicon glyphicon-map-marker" aria-hidden="true"></span>
              <span class="glyphicon-text">Gaborone, Botswana</span>

            </li>

            <li>
              <span class="glyphicon glyphicon glyphicon glyphicon-phone-alt" aria-hidden="true"></span>
              <span class="glyphicon-text">+267 71 526 879</span>
            </li>
            <li>

              <span class="glyphicon glyphicon glyphicon glyphicon-envelope" aria-hidden="true"></span>
              <span class="glyphicon-text">kinyua@mjenzi.com</span>
            </li>
          </ul>


        </div>

        <div class="col-md-6 contact-form">
          <h4><strong>Send us a message</strong></h4>

          <form action="contact_message" method="POST" id="contact_message_form">
            <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
            <div class="row">

              <div class="col-md-6">
                <div class="form-group required">
                  <label for="contact-name">Name</label>
                  <input type="text" name="contact_name" class="form-control" placeholder="Full Name" id="contact-name" required placeholder="">
                </div>

              </div>

              <div class="col-md-6">
                <div class="form-group required">
                  <label for="contact-email">Email address</label>
                  <input type="email" name="contact_email" class="form-control" id="contact-email" required placeholder="Enter email">
                </div>
              </div>
            </div>


            <div class="form-group required">
              <label for="contact-message">Message</label>
              <textarea class="form-control" name="contact_message" id="contact-message" required rows="3"></textarea>
            </div>
            <div style="display:none" class="alert" id="after-send-message" role="alert">...</div>
            <button type="submit" data-loading-text="Please Wait..." class="btn tf-btn btn-default">Submit</button>
          </form>

        </div>


      </div>

    <div class="row">
        <div class="col-md-12 contact-map">

          <style>
            #map {
              height: 400px;
              width: 100%;
            }
          </style>
          <div id="map"></div>
          <script>
            function initMap() {
              var uluru = {lng: 25.8975782, lat: -24.6495848};
              var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 5,
                center: uluru
              });
              var marker = new google.maps.Marker({
                position: uluru,
                map: map
              });
            }
          </script>
          <script async defer
                  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDZ9O1j1GX9FsK2MPNGCAZsJD4LwhUfgW4&callback=initMap">
          </script>

    </div>

  </div>
</div>
